{
  "name": "saas-landing-page-generator",
  "version": "1.0.0",
  "description": "AI-powered automated SaaS landing page generator with A/B testing and analytics",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:e2e": "playwright test",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css}\"",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "analyze": "webpack-bundle-analyzer dist/stats.json",
    "demo": "tsx demo.ts",
    "cli": "tsx cli.ts",
    "quickstart": "tsx quickstart.ts",
    "saas-gen": "tsx cli.ts",
    "deploy-github": "tsx deploy-github.ts",
    "vercel-build": "prisma generate && npm run build",
    "check-deploy": "node check-vercel.js",
    "prebuild": "rimraf dist",
    "postinstall": "prisma generate"
  },
  "keywords": [
    "saas",
    "landing-page",
    "generator",
    "ai",
    "automation",
    "a/b-testing"
  ],
  "author": "Jared",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.20.0",
    "@prisma/client": "^5.12.0",
    "@vercel/analytics": "^1.2.0",
    "axios": "^1.6.8",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.7.0",
    "cheerio": "^1.0.0-rc.12",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.2.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.38.0",
    "playwright": "^1.43.0",
    "sharp": "^0.33.3",
    "stripe": "^15.1.0",
    "uuid": "^9.0.1",
    "winston": "^3.13.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.43.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.12.7",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "prisma": "^5.12.0",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.8",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}